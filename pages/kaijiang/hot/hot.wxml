    <navigator url="dlt/dlt?expect={{dlt.curr_expect?dlt.curr_expect:dlt.kaijiang_expect}}" hover-class="none">
      <view class="kaij-mylist-li">
        <view class="kaij-mylist-lier">
          <view class="kaij-mylist-l">
            <view class="ui-logo ui-logo-dlt"></view>
          </view>
            <view class="kaij-mylist-r">
              <view class="kaij-mylist-hd">
                <view class="itm-time">{{dlt.curr_expect?current_date:dlt.result_time}} {{dlt.curr_expect?current_week:dltWeek}}
